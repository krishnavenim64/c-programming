#include<stdio.h>
#include<stdio.h>
//helper array to revers a part of array 
void revers array (int arr[],int start,int end)
{while (start<end){
int temp=arr[start];
arr[start]=arr[end];
start++;
end--;
}
}
void left rotate (int arr[],int n,int k){
k=k%n;
reverse array(arr,0,k-1);
reverse array(arr,k,n-1);
reverse array (arr,0,n-1);
}
int main(){
int n,k;
printf ("enter N and K:");
scanf ("%d%d",&n,&k);
int*arr=(int*)malloc (n*size of (int));
printf("enter array demarks:");
for (int i=0;i,n;i++){scanf("%d",&arr[i]);
return 0;
}
